cd ../
rm -rf boinc/*
svn co http://boinc.berkeley.edu/svn/trunk/boinc
cd boinc
svn co http://boinc.berkeley.edu/svn/trunk/depends_projects/zip
mv configure.ac configure.ac.new
cp ../cpdnboinc/boinc_configure.ac configure.ac
./_autosetup
./configure --disable-client --disable-server --disable-manager
make clean && make
cd zip
make
cd ../cpdnboinc
